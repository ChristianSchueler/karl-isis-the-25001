# print a lot of stuff to the console and log files
DEBUG="true"

# OpenAI GPT-3 API key goes here. Get it from here: https://beta.openai.com/account/api-keys
OPENAI_API_KEY=""

# OpenAI organization from settings
OPENAI_ORGANIZATION="org-mqENxZ7EqtQbJdDXirypL11N"

# OpenAI used model for inference
OPENAI_MODEL="gpt-4o"

# which kind of requests are being used and which response format, e.g. JSON or comma separated lists
# JsonResult, ListResult, PreventAlcoholicGpt, ChatGpt2024
AI_SYSTEM="StructuredOutput2024"

# how long should we wait at least during querying OpenAI
# default: 5000
OPENAI_MIN_QUERY_DURATION_MS=5000

# start electron in fullscreeen AND kiosk mode
FULLSCREEN="true"

# show devtools in Electron
DEVTOOLS="false"

# hold long user has to hold the button to be registered
BUTTON_MIN_HOLD_DURATION_MS=100

# how long should we wait after showing the recipe
RECIPE_DISPLAY_DURATION_MS=20000
